<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment4">
    <fragment
        android:id="@+id/splashFragment4"
        android:name="com.iti.java.foodplannerbykhalidamr.SplashFragment"
        android:label="SplashFragment" >
        <action
            android:id="@+id/action_splashFragment4_to_authenticationFragment42"
            app:destination="@id/authenticationFragment4" />
        <action
            android:id="@+id/action_splashFragment4_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/authenticationFragment4"
        android:name="com.iti.java.foodplannerbykhalidamr.authentication.AuthenticationFragment"
        android:label="AuthenticationFragment" >
        <action
            android:id="@+id/action_authenticationFragment4_to_emailLoginFragment2"
            app:destination="@id/emailLoginFragment2" />
        <action
            android:id="@+id/action_authenticationFragment4_to_emailSignupFragment"
            app:destination="@id/emailSignupFragment" />
        <action
            android:id="@+id/action_authenticationFragment4_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/emailLoginFragment2"
        android:name="com.iti.java.foodplannerbykhalidamr.authentication.emailAuth.view.EmailLoginFragment"
        android:label="fragment_email_login"
        tools:layout="@layout/fragment_email_login" >
        <action
            android:id="@+id/action_emailLoginFragment2_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/emailSignupFragment"
        android:name="com.iti.java.foodplannerbykhalidamr.authentication.emailAuth.view.EmailSignupFragment"
        android:label="fragment_email_signup"
        tools:layout="@layout/fragment_email_signup" >
        <action
            android:id="@+id/action_emailSignupFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.iti.java.foodplannerbykhalidamr.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
</navigation>